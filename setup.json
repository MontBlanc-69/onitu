{
  "name": "example",

  "entries": {
    "A": {
      "driver": "local_storage",
      "options": {
        "root": "example/driver_a"
      }
    },
    "B": {
      "driver": "local_storage",
      "options": {
        "root": "example/driver_b"
      }
    },
    "dropbox": {
      "driver": "dropbox",
      "options": {
        "root": "/onitu/",
        "access_key": "Your_Access_Key",
        "access_secret": "Your_Access_Secret"
      }
    }
  },

  "rules": [
    {
      "match": {"path": "/"},
      "sync": ["A"]
    },
    {
      "match": {"path": "/backedup/", "mime": ["application/pdf"]},
      "sync": ["B"]
    },
    {
      "match": {"path": "/dropbox/"},
      "sync": ["dropbox"]
    }
  ]

}
